Private Sub cmdImp_Click()
Dim Param(3) As Variant
Dim formulas(2) As Variant
On Error GoTo errores

If Rs.EOF And Rs.BOF Then
    MsgBox "No hay Pedidos a imprimir !!!", vbInformation, "Sistemas"
    Ctr_almacen.SetFocus
    Exit Sub
End If
                                   
Screen.MousePointer = 11

Param(0) = VGParamSistem.BDEmpresa
Param(1) = VGparametros.empresacodigo
Param(2) = IIf(Len(Trim(Ctr_almacen.xclave)) <> 0, Trim(Ctr_almacen.xclave), "%")

formulas(0) = "@Empresa='" & VGparametros.NomEmpresa & "'"
formulas(0) = "@ruc='" & VGparametros.RucEmpresa & "'"

Call ImpresionRptProc("vt_formato_produccion.rpt", formulas, Param, , "Pedido A Producccion")
                                   
Screen.MousePointer = 1



Exit Sub
errores:
  Screen.MousePointer = 1
  MsgBox "Error inesperado: " & Err.Number & "  " & Err.Description, vbExclamation
  Err = 0
  Exit Sub
End Sub

