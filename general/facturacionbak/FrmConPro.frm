& "(SELECT STALMA,STCODIGO,STSKDIS,ENTREGADO=0  FROM STKART inner join maeart on stcodigo=acodigo " _
& " where afstock = 1 Union All " _
& " SELECT ALMACENCODIGO,PRODUCTOCODIGO,0,DIS=DETPEDCANTPEDIDA-ISNULL(DECANTID,0) " _
& " FROM V_ALMACENYVENTAS) Z " _
& " " & SQL & "  GROUP BY Z.STALMA,Z.STCODIGO order by z.stalma ")
